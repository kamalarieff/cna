{
  "name": "@mudahmy/mudah-cna",
  "version": "1.0.23",
  "description": "CLI for creating next.js apps boilerplate",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier --trailing-comma es5 --single-quote --write 'bin/**/*.js'",
    "pub": "npm version patch && npm publish"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "bin": {
    "mudah-cna": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab2.mudah.my/kamal/mudah-cna"
  },
  "keywords": [
    "nextjs",
    "boilerplate",
    "stater-kit",
    "cli",
    "redux",
    "reactjs",
    "flux",
    "styled-components",
    "typescript"
  ],
  "authors": [
    "Kamal Arieff <kamal.arieff@mudah.my> (https://github.com/davidnguyen179)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab2.mudah.my/kamal/mudah-cna/issues"
  },
  "homepage": "https://gitlab2.mudah.my/kamal/mudah-cna",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "fs-extra": "^7.0.1",
    "plop": "^2.3.0",
    "semver": "^6.0.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.2.2",
    "prettier": "^1.6.1"
  }
}
